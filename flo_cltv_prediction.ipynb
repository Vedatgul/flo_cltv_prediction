   "metadata": {},
   "outputs": [],
   "source": [
    "# recency: Time, since last user-specific purchase on a weekly basis\n",
    "# T: how long before the first purchase was made the analysis date (the age of the customer on a weekly basis)\n",
    "# frequency: total number of repeat purchases (frequency>1)\n",
    "# monetary: average earnings per purchase"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "d320571e",
   "metadata": {},
   "outputs": [],
   "source": [
    "cltv_df['recency'] = (cltv_df['last_order_date'] - cltv_df['first_order_date']).dt.days / 7"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "9f1171d4",
   "metadata": {},
   "outputs": [],
   "source": [
    "cltv_df['T'] = (today_date - cltv_df['first_order_date']).dt.days / 7"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "id": "d75e2b68",
   "metadata": {},
   "outputs": [],
   "source": [
    "cltv_df.rename(columns={'order_num_total': 'frequency'}, inplace=True)\n",
    "cltv_df = cltv_df[cltv_df['frequency'] > 1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "0a1ecd93",
   "metadata": {},
   "outputs": [],
   "source": [
    "cltv_df['monetary'] = cltv_df['customer_value_total'] / cltv_df['frequency']"
   ]
  },
